{
  "tags": {
    "allowUnknownTags": true
  },
  "plugins": ["plugins/markdown"],

  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false,
    "dateFormat": "ddd MMM Do YYYY",
    "outputSourceFiles": true,
    "outputSourcePath": true,
    "systemName": "TextHighlighter",
    "footer": "",
    "copyright": "TextHighlighter Copyright Â© 2019 by Perlego Limited.",
    "navType": "inline",
    "theme": "cosmo",
    "linenums": true,
    "collapseSymbols": false,
    "inverseNav": true,
    "highlightTutorialCode": true,
    "protocol": "fred://"
  },
  "markdown": {
    "parser": "gfm",
    "hardwrap": true
  }
}
